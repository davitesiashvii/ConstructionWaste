@model ConstructionWaste.DTOs.Clients.ClientTest

@{
    ViewData["Title"] = "EditClient";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>EditClient</h1>

<h4>ClientTest</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditClient">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Phone" class="control-label"></label>
                <input asp-for="Phone" class="form-control" />
                <span asp-validation-for="Phone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Representative" class="control-label"></label>
                <input asp-for="Representative" class="form-control" />
                <span asp-validation-for="Representative" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdentityCode" class="control-label"></label>
                <input asp-for="IdentityCode" class="form-control" />
                <span asp-validation-for="IdentityCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InsertTime" class="control-label"></label>
                <input asp-for="InsertTime" class="form-control" />
                <span asp-validation-for="InsertTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

